{"ast":null,"code":"import axios from \"axios\";\nimport request from \"@/utils/request\";\nexport default {\n  name: 'HomeView',\n  data() {\n    return {\n      isCollapse: false,\n      //不收缩\n      asideWidth: '200px',\n      collapseIcon: 'el-icon-s-fold',\n      users: []\n    };\n  },\n  mounted() {\n    // axios.get('http://localhost:9090/user/selectAll').then(res => {\n    //   console.log(res.data) //后台返回的数据\n    //   this.users = res.data.data\n    // })\n    request.get('user/selectAll').then(res => {\n      this.users = res.data;\n    });\n  },\n  methods: {\n    handleCollapse() {\n      this.isCollapse = !this.isCollapse, this.asideWidth = this.isCollapse ? '64px' : '200px', this.collapseIcon = this.isCollapse ? 'el-icon-s-unfold' : 'el-icon-s-fold';\n    }\n  }\n};","map":{"version":3,"names":["axios","request","name","data","isCollapse","asideWidth","collapseIcon","users","mounted","get","then","res","methods","handleCollapse"],"sources":["src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div>\n  <el-container>\n    <!--侧边栏-->\n    <el-aside style=\"min-height: 100vh; background-color: #001529\"\n    :width=\"asideWidth\">\n      <div style=\"height: 100px; color: white;\n      display: flex; align-items: center; justify-content: center\">\n      <img src=\"@/assets/logo1.png\" alt=\"\" style=\"width: 40px; height: 40px\">\n          <span class=\"logo-title\" v-show=\"!isCollapse\">honey2024</span>\n      </div>\n      <el-menu router style=\"border: none\" background-color=\"#001529\"\n               text-color=\"rgba(255,255,255,0.85)\" active-text-color=\"#fff\"\n               :default-active=\"$route.path\" :collapse=\"isCollapse\" :collapse-transition=\"false\">\n        <el-menu-item index=\"/\">\n            <i class=\"el-icon-house\"></i>\n            <span slot=\"title\">系统首页</span>\n        </el-menu-item>\n        <el-menu-item index=\"/1\">\n            <i class=\"el-icon-house\"></i>\n            <span slot=\"title\">系统首页</span>\n        </el-menu-item>\n        <el-menu-item index=\"/2\">\n          <i class=\"el-icon-house\"></i>\n          <span slot=\"title\">系统首页</span>\n      </el-menu-item>\n        <el-submenu>\n          <template slot=\"title\">\n            <i class=\"el-icon-menu\"></i>\n            <span>学生系统</span>\n          </template>\n          <el-menu-item>学生信息</el-menu-item>\n          <el-menu-item>教师信息</el-menu-item>\n        </el-submenu>\n      </el-menu>\n    </el-aside>\n\n\n    <el-container>\n      <!--头部区域-->\n      <el-header style=\"height: 100px\">\n          <i :class=\"collapseIcon\" style=\"font-size: 30px\" @click=\"handleCollapse\"></i>\n        <el-breadcrumb separator-class=\"el-icon-arrow-right\" style=\"margin-left: 20px\">\n          <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\n          <el-breadcrumb-item :to=\"{ path: '/user' }\">用户管理</el-breadcrumb-item>\n        </el-breadcrumb>\n\n        <div style=\"flex: 1; width: 0; display: flex; align-items: center; justify-content: flex-end\">\n          <el-dropdown placement=\"bottom\">\n            <div style=\"display: flex; align-items: center; cursor: default\">\n              <img src=\"@/assets/logo1.png\" alt=\"\" style=\"width: 40px; height: 40px\">\n              <span>管理员</span>\n            </div>\n            <el-dropdown-menu slot=\"dropdown\">\n              <el-dropdown-item>个人信息</el-dropdown-item>\n              <el-dropdown-item>修改密码</el-dropdown-item>\n              <el-dropdown-item @click.native=\"$router.push('/login')\">退出登录</el-dropdown-item>\n            </el-dropdown-menu>\n          </el-dropdown>\n        </div>\n\n      </el-header>\n      <!--主体区域-->\n      <el-main>\n        <div style=\"box-shadow: 0 0 10px rgba(0,0,0,.1); padding: 10px 20px; border-radius: 5px; border-radius: 5px; margin-bottom: 10px\">\n          你好，Chan9zero！\n        </div>\n        <div style=\"display: flex\">\n          <el-card style=\"width: 50%; margin-right: 10px\">\n            <div slot=\"header\" class=\"clearfix\">\n              <span>学习毕设2024</span>\n            </div>\n            <div>\n              2024毕设正式开始了!\n              <div style=\"margin-top: 20px\">\n                <div style=\"margin: 10px 0\"><strong>主题色</strong></div>\n                <el-button type=\"primary\">按钮</el-button>\n                <el-button type=\"success\">按钮</el-button>\n                <el-button type=\"warning\">按钮</el-button>\n                <el-button type=\"danger\">按钮</el-button>\n                <el-button type=\"info\">按钮</el-button>\n              </div>\n            </div>\n          </el-card>\n          <el-card style=\"width: 50%\">\n            <div slot=\"header\" class=\"clearfix\">\n              <span>学习毕设2024</span>\n            </div>\n            <div>\n              <el-table :data=\"users\">\n                <el-table-column label=\"ID\" prop=\"id\"></el-table-column>\n                <el-table-column label=\"用户名\" prop=\"username\"></el-table-column>\n                <el-table-column label=\"姓名\" prop=\"name\"></el-table-column>\n                <el-table-column label=\"地址\" prop=\"address\"></el-table-column>\n              </el-table>\n            </div>\n          </el-card>\n        </div>\n<!--        主体区域-->\n      </el-main>\n    </el-container>\n  </el-container>\n  </div>\n</template>\n\n<script>\n\nimport axios from \"axios\";\nimport request from \"@/utils/request\";\nexport default {\n  name: 'HomeView',\n  data(){\n    return{\n      isCollapse: false, //不收缩\n      asideWidth: '200px',\n      collapseIcon: 'el-icon-s-fold',\n      users: []\n    }\n  },\n  mounted() {\n    // axios.get('http://localhost:9090/user/selectAll').then(res => {\n    //   console.log(res.data) //后台返回的数据\n    //   this.users = res.data.data\n    // })\n    request.get('user/selectAll').then(res => {\n      this.users = res.data\n    })\n  },\n  methods: {\n    handleCollapse(){\n      this.isCollapse = !this.isCollapse,\n          this.asideWidth = this.isCollapse ? '64px' : '200px',\n          this.collapseIcon = this.isCollapse ? 'el-icon-s-unfold' : 'el-icon-s-fold'\n    }\n  }\n}\n</script>\n\n\n<style>\n.el-menu--inline{\n  background-color: #000c17 !important;\n}\n.el-menu--inline .el-menu-item{\n  background-color: #000c17 !important;\n  padding-left: 49px;\n}\n.el-menu-item:hover, .el-submenu__title:hover{\n  color: #fff !important;\n}\n.el-menu-item.is-active{\n  background-color: #40a9ff !important;\n  border-radius: 10px;\n}\n.el-menu-item{\n  height: 40px;\n  line-height: 40px;\n}\n.el-submenu__title{\n  height: 40px;\n  line-height: 40px;\n}\n.el-aside{\n  transition: width.3s;\n  box-shadow: 2px 0 6px rgba(0,21,41,.35);\n}\n.logo-title{\n  margin-left: 5px;\n  font-size: 20px;\n  transition: all .3s;   /* 0.3s*/\n}\n.el-header{\n  box-shadow: 2px 0 6px rgba(0,21,41,.35);\n  display: flex;\n  align-items: center;\n}\n</style>"],"mappings":"AA2GA,OAAAA,KAAA;AACA,OAAAC,OAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,UAAA;MAAA;MACAC,UAAA;MACAC,YAAA;MACAC,KAAA;IACA;EACA;EACAC,QAAA;IACA;IACA;IACA;IACA;IACAP,OAAA,CAAAQ,GAAA,mBAAAC,IAAA,CAAAC,GAAA;MACA,KAAAJ,KAAA,GAAAI,GAAA,CAAAR,IAAA;IACA;EACA;EACAS,OAAA;IACAC,eAAA;MACA,KAAAT,UAAA,SAAAA,UAAA,EACA,KAAAC,UAAA,QAAAD,UAAA,qBACA,KAAAE,YAAA,QAAAF,UAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}