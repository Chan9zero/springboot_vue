"电话\" prop=\"phone\">\r\n          <el-input v-model=\"user.phone\" placeholder=\"电话\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"邮箱\" prop=\"email\">\r\n          <el-input v-model=\"user.email\" placeholder=\"邮箱\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"地址\" prop=\"address\">\r\n          <el-input type=\"textarea\" v-model=\"user.address\" placeholder=\"地址\"></el-input>\r\n        </el-form-item>\r\n        <div style=\"text-align: center; margin-bottom: 20px\">\r\n          <el-button size=\"small\" type=\"primary\" @click=\"updata\">保 存</el-button>\r\n        </div>\r\n      </el-form>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Person\",\r\n  data(){\r\n    return {\r\n      user: JSON.parse(localStorage.getItem('honey-user') || '{}')\r\n    }\r\n  },\r\n  methods:{\r\n    update() {\r\n      this.$request.put('/user/update', this.user).then(res =>{\r\n        if(res.code === '200'){\r\n          //成功更新\r\n          this.$message.success('保存成功')\r\n          localStorage.getItem('honey-user', JSON.stringify(this.user))\r\n        } else{\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    handleAvatarSuccess(reponse, file, fileList){\r\n      this.user.avatar = reponse.data\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/deep/.el-form-item__label{\r\n  font-weight: bold;\r\n}\r\n/deep/.el-upload{\r\n  border-radius: 50%;\r\n}\r\n.avatar-uploader .el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: #409EFF;\r\n}\r\n.avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 178px;\r\n  height: 178px;\r\n  line-height: 178px;\r\n  text-align: center;\r\n  border-radius: 50%;\r\n}\r\n.avatar {\r\n  width: 178px;\r\n  height: 178px;\r\n  display: block;\r\n  border-radius: 50%;\r\n}\r\n\r\n</style>"],"mappings":"AAyCA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IACA;EACA;EACAC,OAAA;IACAC,OAAA;MACA,KAAAC,QAAA,CAAAC,GAAA,sBAAAR,IAAA,EAAAS,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA;UACA,KAAAC,QAAA,CAAAC,OAAA;UACAV,YAAA,CAAAC,OAAA,eAAAH,IAAA,CAAAa,SAAA,MAAAd,IAAA;QACA;UACA,KAAAY,QAAA,CAAAG,KAAA,CAAAL,GAAA,CAAAM,GAAA;QACA;MACA;IACA;IACAC,oBAAAC,OAAA,EAAAC,IAAA,EAAAC,QAAA;MACA,KAAApB,IAAA,CAAAqB,MAAA,GAAAH,OAAA,CAAAnB,IAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}